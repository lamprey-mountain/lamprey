{
  "db_name": "PostgreSQL",
  "query": "\n        \tSELECT\n            \tr.id,\n            \tr.description,\n            \tr.is_mentionable,\n            \tr.allow as \"allow: _\",\n            \tr.deny as \"deny: _\",\n            \tr.version_id,\n            \tr.room_id,\n            \tr.is_self_applicable,\n            \tr.name,\n                coalesce(rm.count, 0) as \"member_count!\",\n                r.position,\n                r.hoist\n            FROM role r\n            LEFT JOIN (\n                SELECT role_id, count(*) as count\n                FROM role_member\n                GROUP BY role_id\n            ) rm ON rm.role_id = r.id\n        \tWHERE r.room_id = $1 AND r.id > $2 AND r.id < $3\n        \tORDER BY (CASE WHEN $4 = 'f' THEN r.id END), r.id DESC LIMIT $5\n            ",
