{
  "db_name": "PostgreSQL",
  "query": "WITH RECURSIVE parents AS (\n    SELECT id, reply_id, 0 as level\n    FROM message\n    WHERE id = $1 AND is_latest\n    UNION ALL\n    SELECT m.id, m.reply_id, p.level + 1\n    FROM message m\n    JOIN parents p ON m.id = p.reply_id\n    WHERE m.is_latest AND p.level < $2\n)\nSELECT\n    msg.type as \"message_type: DbMessageType\",\n    msg.id,\n    msg.channel_id,\n    msg.version_id,\n    msg.ordering,\n    msg.content,\n    msg.metadata,\n    msg.reply_id,\n    msg.override_name,\n    msg.author_id,\n    msg.created_at,\n    msg.edited_at,\n    msg.deleted_at,\n    msg.removed_at,\n    msg.pinned,\n    hm.mentions,\n    coalesce(att_json.attachments, '{}') as \"attachments!\",\n    msg.embeds as \"embeds\"\nFROM message as msg\nJOIN parents p ON msg.id = p.id\nLEFT JOIN att_json ON att_json.version_id = msg.version_id\nLEFT JOIN hydrated_mentions hm ON hm.message_id = msg.id\nWHERE msg.is_latest\n  AND msg.deleted_at IS NULL\n  AND msg.id != $1\nORDER BY p.level ASC",
