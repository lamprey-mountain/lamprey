  "db_name": "PostgreSQL",
  "query": "SELECT\n    msg.type as \"message_type: DbMessageType\",\n    msg.id,\n    msg.channel_id,\n    msg.version_id,\n    msg.ordering,\n    msg.content,\n    msg.metadata,\n    msg.reply_id,\n    msg.override_name,\n    msg.author_id,\n    msg.created_at,\n    msg.edited_at,\n    msg.deleted_at,\n    msg.removed_at,\n    msg.pinned,\n    hm.mentions,\n    coalesce(att_json.attachments, '{}') as \"attachments!\",\n    msg.embeds as \"embeds\"\nfrom message as msg\nleft join att_json on att_json.version_id = msg.version_id\nleft join hydrated_mentions hm on hm.message_id = msg.id\nwhere is_latest and channel_id = $1 and msg.id = $2 and msg.deleted_at is null\n",
